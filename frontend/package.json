		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-svelte": "^2.35.1",
		"eslint-plugin-unicorn": "^50.0.1",
		"postcss": "^8.4.32",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.6.2",
