{
  "name": "sim-pipe-simulation-controller",
  "version": "0.0.1",
  "description": "Simulations and analytics for DataCloud pipelines.",
  "exports": "./dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./src/index.ts",
    "sftp-utils": "node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./src/sftp-utils.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .ts src",
    "build": "tsc",
    "start-compiled": "node --experimental-specifier-resolution=node ./dist/index.js",
    "hasura-console": "npx hasura-cli console --project hasura",
    "download-db-schema": "npx @apollo/rover graph introspect http://localhost:8080/v1/graphql --header x-hasura-admin-secret:hasuraadminsecret > src/db/schema.graphql",
    "codegen-db": "graphql-codegen --config src/db/codegen.yaml"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DataCloud-project/SIM-PIPE-Simulation-Controller.git"
  },
  "author": "SINTEF",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/DataCloud-project/SIM-PIPE-Simulation-Controller/issues"
  },
  "homepage": "https://github.com/DataCloud-project/SIM-PIPE-Simulation-Controller#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-graphql-request": "^4.3.3",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@types/config": "^0.0.40",
    "@types/dockerode": "^3.3.0",
    "@types/node": "^17.0.5",
    "@types/set-interval-async": "^1.0.0",
    "@types/ssh2-sftp-client": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "hasura-cli": "^2.7.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "apollo-server": "^3.6.1",
    "apollo-server-express": "^3.10.0",
    "dockerode": "^3.3.1",
    "dotenv": "^14.2.0",
    "expiry-map": "^2.0.0",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "got": "^12.1.0",
    "graphql": "^16.2.0",
    "graphql-request": "^3.7.0",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "jwt-decode": "^3.1.2",
    "p-memoize": "^7.1.0",
    "set-interval-async": "^2.0.3",
    "ssh2-sftp-client": "^7.2.1",
    "winston": "^3.3.3"
  }
}
